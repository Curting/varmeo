                        <%= link_to "Mitsubishi - Hero",
                                    hero_path,
                                    class: "flex items-center gap-2 w-full first-of-type:rounded-t-md last-of-type:rounded-b-md text-gray-700 px-4 py-2.5 text-left text-sm hover:bg-gray-50" %>

                        <%= link_to "Panasonic - Flagskib",
                                    flagskib_path,
                                    class: "flex items-center gap-2 w-full first-of-type:rounded-t-md last-of-type:rounded-b-md text-gray-700 px-4 py-2.5 text-left text-sm hover:bg-gray-50" %>

                        <%= link_to "Køb eller abonnement?",
                                    luft_til_luft_path,
